#               Quote handling
# ---------------------------------------------------
query I
SELECT CAST('[''hello'',''world'', ''!'']' AS VARCHAR[]);
----
['hello', 'world', '!']

query I
SELECT CAST('[''''hello'''',''''world'''', ''''!'''']' AS VARCHAR[]);
----
[''hello'', ''world'', ''!'']

query I
SELECT CAST('[[ ['', , '']], [[duck, db, ''''] ]]' AS VARCHAR[][][]);
----
[[[', , ']], [[duck, db, '']]]

query I
SELECT CAST('["can''t", "you''re", "i''m"]' AS VARCHAR[]);
----
["can't", "you're", "i'm"]

query I
SELECT CAST('[can''t, you''re, i''m]' AS VARCHAR[]);
----
[can't, you're, i'm]

query I
SELECT CAST('["]", "hello", "world"]' AS VARCHAR[]);
----
["]", "hello", "world"]

query I
SELECT CAST('['']'', "hello", "world"]' AS VARCHAR[]);
----
[']', "hello", "world"]


