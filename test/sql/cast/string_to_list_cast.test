statement  ok
CREATE TABLE stringList (col1 VARCHAR)

statement ok
INSERT INTO stringList VALUES ('["hello","world","!"]'), ('["Amazing","text"]'), ('[Hello World!]');

query I
SELECT col1::VARCHAR[] FROM stringList;
----
["hello", "world", "!"]
["Amazing", "text"]
[Hello World!]

