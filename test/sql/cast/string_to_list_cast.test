----
["hello", "world", "!"]

query I
SELECT CAST('[Hello World!]' AS VARCHAR[]);
----
[Hello World!]

query I
SELECT CAST('[[Hello World!], hello, universe]' AS VARCHAR[]);
----
[[Hello World!], hello, universe]

query I
SELECT '[Cast like this]':: VARCHAR[];
