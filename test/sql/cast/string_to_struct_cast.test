#               Struct with lists
# ---------------------------------------------------
query I
SELECT '{key_A: [2, 3, 4], key_B: [Hello, World]}'::STRUCT(key_A INT[], key_B VARCHAR[]);
----
{'key_A': [2, 3, 4], 'key_B': [Hello, World]}

query I
SELECT '{key_A: [[2, 3], [4]], key_B: [Hello, World]}'::STRUCT(key_A INT[][], key_B VARCHAR[]);
----
{'key_A': [[2, 3], [4]], 'key_B': [Hello, World]}


