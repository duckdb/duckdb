#
# Coverage tests
#

# ReuseIndexes, backwards moving frame
query II
WITH t(i, p, f) AS (VALUES
	(0, 1, 1),
	(1, 1, 1),
	(2, 1, 1),
	(3, 3, 1),
	(4, 1, 1),
	(5, 3, 1)
)
SELECT i, MEDIAN(i) OVER (ORDER BY i ROWS BETWEEN p PRECEDING and f FOLLOWING)
FROM t
ORDER BY 1
----
0	0.500000
1	1.000000
2	2.000000
3	2.000000
4	4.000000
5	3.500000

# CanReplace
query II
WITH t(r, i, p, f) AS (VALUES
	(0, 0, 1, 1),
	(1, 1, 1, 1),
	(2, 2, 1, 1),
	(3, 0, 1, 1),
	(4, 1, 1, 1),
	(5, 2, 1, 1)
)
SELECT r, MEDIAN(i) OVER (ORDER BY r ROWS BETWEEN p PRECEDING and f FOLLOWING)
FROM t
ORDER BY 1
----
0	0.500000
1	1.000000
2	1.000000
3	1.000000
4	1.000000
5	1.500000

# Moving discrete list
query II
WITH t(r, i, p, f) AS (VALUES
	(0, 0, 1, 2),
	(1, 1, 1, 2),
	(2, 2, 1, 2),
	(3, 3, 1, 2),
	(4, 4, 1, 2),
	(5, 5, 1, 2)
)
SELECT r, QUANTILE_DISC(i, [0.25, 0.5, 0.75]) OVER (ORDER BY r ROWS BETWEEN p PRECEDING and f FOLLOWING)
FROM t
ORDER BY 1
----
0	[0, 1, 1]
1	[0, 1, 2]
2	[1, 2, 3]
3	[2, 3, 4]
4	[3, 4, 4]
5	[4, 4, 4]

