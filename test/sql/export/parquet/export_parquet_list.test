# name: test/sql/export/parquet/export_parquet_list.test
# description: Test EXPORT DATABASE with LIST columns
# group: [parquet]

require parquet

statement ok
begin transaction;

statement ok
create table tbl as select val from (
	select ['01010101'::BIT, '01011101001'::BIT] val
), range(3);

query I nosort result
select * from tbl;
----

# now export the db
statement ok
EXPORT DATABASE '{TEMP_DIR}/export_test' (FORMAT PARQUET)

statement ok
ROLLBACK

statement ok
IMPORT DATABASE '{TEMP_DIR}/export_test'

query I nosort result
select * from tbl;
----
