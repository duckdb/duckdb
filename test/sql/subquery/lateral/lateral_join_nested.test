statement ok
create view strings as (with recursive t(a) as (select 1 union select a+1 from t where a < 5) select * from t order by a);

query I
select sum(a)
from strings
where a in (select k
            from (select 1) t(l) ,
                 (select 1+l) t2(j),
                 (select a-j
                  from strings) t3(k));
----
6

statement ok
create view strings2 as (with recursive t2(b) as (select 0 union select 10*a from strings where a < 5) select * from t2 order by b);

query I
select *
from strings2
where b in (select k
            from (select 10) t(l) ,
                 (select 10+l) t2(j),
                 (select l+10*a
                  from strings) t3(k))
group by b
having b%20 == 0
order by b desc;
----
40
20
