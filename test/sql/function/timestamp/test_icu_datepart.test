	('2021-11-15 02:30:00-07'::TIMESTAMPTZ, 'minute'),
	('2021-12-25 00:00:00+02'::TIMESTAMPTZ, 'day'),
	('infinity'::TIMESTAMPTZ, 'second'),
	('-infinity'::TIMESTAMPTZ, 'decade'),
