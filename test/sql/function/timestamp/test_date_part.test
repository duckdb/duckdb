# Invalid parts
statement error
SELECT ts, DATE_PART(['duck', 'month', 'day'], ts) AS parts
FROM timestamps
ORDER BY 1;
----

# Duplicate parts
statement error
SELECT ts, DATE_PART(['year', 'month', 'day', 'year'], ts) AS parts
FROM timestamps
ORDER BY 1;
----

# Empty parts
statement error
SELECT DATE_PART([], ts) FROM timestamps;
----

# Null parts
statement error
SELECT DATE_PART(['year', NULL, 'month'], ts) FROM timestamps;
----

