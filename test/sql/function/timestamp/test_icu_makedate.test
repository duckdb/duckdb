# TZ lookup table
statement ok
CREATE TABLE timezones AS (SELECT mm, tz FROM (VALUES
	(1, 'America/New_York'),
	(2, 'America/Los_Angeles'),
	(3, 'Europe/Rome'),
	(4, 'Asia/Kathmandu'),
	(5, 'Canada/Newfoundland'),
	(7, 'Pacific/Auckland'),
	(8, 'Asia/Hong_Kong'),
	(12, 'US/Hawaii')
	) tbl(mm, tz)
);

statement ok
CREATE TABLE timeparts AS (
	SELECT ts,
		yeartz(ts) yyyy,
		month(ts) mm,
		day(ts) dd,
		hour(ts) hr,
		minute(ts) mn,
		microsecond(ts) / 1000000.0 as ss,
		tz
	FROM timestamps t
	LEFT JOIN timezones z ON (month(t.ts) = z.mm)
);

