# name: test/sql/function/timestamp/test_strptime.test
# description: Test strptime function
# group: [timestamp]

statement ok
PRAGMA enable_verification

query I
SELECT strptime('21 June, 2018', '%d %B, %Y')
----
2018-06-21 00:00:00

query I
