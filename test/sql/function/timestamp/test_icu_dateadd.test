# Backward from the maximum
# UDate loses ms accuracy when close to the upper limit,
# so these results are off by 1ms

query II
SELECT iv, ts + iv FROM limits, intervals
WHERE label = 'tsmax'
  AND iv < (INTERVAL (-1) millisecond);
