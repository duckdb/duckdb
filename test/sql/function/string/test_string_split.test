# test unnesting of null values a bit
query T
SELECT string_split(NULL, NULL)
----
NULL

query T
SELECT * FROM (VALUES (string_split('hello world', ' ')), (string_split(NULL, ' ')), (string_split('a b c', NULL)), (string_split('a b c', ' '))) tbl(i)
----
[hello, world]
NULL
[a b c]
[a, b, c]

