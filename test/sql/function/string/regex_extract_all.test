# Different size patterns
query I
WITH t(pattern) AS (
    VALUES
		(NULL),
		('(a)'),
		('(a)(a)(a)'),
		('()'),
		('(a)(a)'),
		(NULL),
		('(a)'),
		('(a)(b)?(a)'),
		('(a)(a)(a)(a)(a)(a)(a)'),
		(NULL),
)
select regexp_extract_all('aaaaaaaa', pattern) from t;
----
NULL
[a, a, a, a, a, a, a, a]
[aaa, aaa]
[, , , , , , , , ]
[aa, aa, aa, aa]
NULL
[a, a, a, a, a, a, a, a]
[aa, aa, aa, aa]
[aaaaaaa]
NULL

