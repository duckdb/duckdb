# "Empty string and prefix"
query T
SELECT prefix('', 'aaa')
----
0

query T
SELECT prefix('aaa', '')
----
1

# "Issue #572 alloc exception on empty table"
statement ok
CREATE TABLE t0(c0 VARCHAR)

query T
SELECT * FROM t0 WHERE PREFIX(t0.c0, '')
----

