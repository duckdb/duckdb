# very large lists

statement ok
CREATE TABLE lists AS SELECT range % 4 g, list(range) l FROM range(6000) GROUP BY range % 4;

query T
with cte0 as (
  select g, list_select(l, range(5)) l from lists
), cte1 as (
  select g, unnest(l) i from cte0
)
select count(*)
from cte1
----
20

