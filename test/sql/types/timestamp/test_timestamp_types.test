# TIME conversions are now supported
query I
select sec::TIME from timestamp;
----
00:00:01
00:00:51
00:00:11

query I
select milli::TIME from timestamp;
----
00:00:01.594
00:00:01.894
00:00:01.794

query I
select nano::TIME from timestamp;
----
00:00:01.889268
00:00:01.999268
00:00:01.899268

# Direct timestamp promotions
foreach src sec milli micro nano

foreach dst sec milli micro nano

statement ok
SELECT ${src}, ${dst} 
FROM timestamp 
WHERE ${src} = ${dst} ;

endloop

endloop

# Cast to DATE
foreach src sec milli micro nano

query I
SELECT ${src}::DATE from timestamp
----
2008-01-01
2008-01-01
2008-01-01

endloop

