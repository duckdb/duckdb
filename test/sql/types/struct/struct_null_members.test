# String STRUCTs with NULLs in various positions
statement ok
CREATE VIEW list_str AS
SELECT * FROM (VALUES
	({'x': 'duck', 'y': ''}),
	({'x': 'duck', 'y': 'goose'}),
	({'x': 'duck', 'y': NULL}),
	({'x': NULL, 'y': 'goose'}),
	({'x': NULL, 'y': NULL}),
	({'x': NULL, 'y': '0'}),
	(NULL)
) tbl(i);

query IITTTTTTTT rowsort
