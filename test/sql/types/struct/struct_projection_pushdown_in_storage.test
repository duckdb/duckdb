	({'a': 1, 'b': true, 'c': 'test'}),
	({'a': 2, 'b': false, 'c': 'hello'}),
	(NULL),
	({'a': 3, 'b': true, 'c': 'this is a long string'}),
	({'a': NULL, 'b': NULL, 'c': NULL});

query I
SELECT i.a FROM test_structs;
----
1
2
NULL
3
NULL

statement ok
pragma explain_output = optimized_only

# The 'struct_extract' expression is entirely removed
query II
