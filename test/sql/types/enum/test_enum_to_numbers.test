statement ok
insert into t2 values  ('2'), ('3'), ('4');

# null to Varchar cast results in NULL
# NULL != '2' results in NULL and is not included
query II
select * from t1, t2 where t2.a NOT IN ('2', '3', '4') order by t1.a, t2.a NULLS FIRST;
