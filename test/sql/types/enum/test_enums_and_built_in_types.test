# name: test/sql/types/enum/test_enums_and_built_in_types.test
# description: ENUM types and built-in types
# group: [enum]

load :memory:

statement ok
PRAGMA enable_verification

statement error
CREATE TYPE "integer" AS ENUM ('1', '2', '3');
----
<REGEX>:.*Catalog Error.*already exists.*

restart

# use a type that is almost a built-in type
statement error
SELECT 4::INTEGEE
----
<REGEX>:.*Catalog Error.*does not exist.*

restart

# cannot drop built-in types
statement error
DROP TYPE "INTEGER"
----
<REGEX>:.*Catalog Error.*Cannot drop internal catalog entry.*

statement error
DROP TYPE "INTEGEE"
----
<REGEX>:.*Catalog Error.*does not exist.*

restart

# cannot create built-in type
statement error
CREATE TYPE integer AS ENUM ('1', '2', '3');
----
<REGEX>:.*Catalog Error.*already exists.*