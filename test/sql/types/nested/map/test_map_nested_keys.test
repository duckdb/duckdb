# name: test/sql/types/nested/map/test_map_nested_keys.test
# description: Test nested types as keys for maps
# group: [map]

statement ok
PRAGMA enable_verification

# Nested keys (list) succesfull
query I
SELECT MAP(
	[
		[1],[2],[3]
	],
	[
		4,2,0
	]
);
----
{[1]=4, [2]=2, [3]=0}

statement ok
CREATE TABLE tbl(a INT[][], b VARCHAR[]);

statement ok
INSERT INTO tbl VALUES([[2],[3],[4]], ['a', 'b', 'c']);

statement ok
INSERT INTO tbl VALUES([[5],[6],[7]], ['d', 'e', 'f']);

statement ok
INSERT INTO tbl VALUES([[8],[9],[10]], ['g', 'h', 'i']);

query I
SELECT MAP(a, b) from tbl;
----
{[2]=a, [3]=b, [4]=c}
{[5]=d, [6]=e, [7]=f}
{[8]=g, [9]=h, [10]=i}

# Nested keys (list) not succesfull (duplicate)
statement error
SELECT MAP(
	[
		[1,2],[2,1],[3,1],[4,2],[4,2,0],[1,2]
	],
	[
		NULL,NULL,NULL,NULL,NULL,NULL
	]
);

# Nested keys (list) not succesfull (nulls)
statement error
SELECT MAP(
	[
		[1,2],[2,1],[3,1],[4,2],[4,2,0],NULL
	],
	[
		NULL,NULL,NULL,NULL,NULL,NULL
	]
);

# Nested keys (struct) succesfull
query I
SELECT MAP(
	[
		{'foo': True}, {'foo': False}, {'foo': NULL}
	],
	[
		4,2,0
	]
);
----
{{'foo': true}=4, {'foo': false}=2, {'foo': NULL}=0}

# Nested keys (struct) not succesfull (duplicate)
statement error
SELECT MAP(
	[
		{'foo': True}, {'foo': False}, {'foo': NULL}, {'foo': True}
	],
	[
		'n', 'o', 'p', 'e'
	]
);

# Nested keys (struct) not succesfull (null)
statement error
SELECT MAP(
	[
		{'foo': 0}, {'foo': 1}, NULL, {'foo': 2}, {'foo': 3}
	],
	[
		'e', 'r', 'r', 'o', 'r'
	]
);

# Nested keys (map) succesfull
query I
SELECT MAP(
	[
		MAP([5],[4]), MAP([10],[2]), MAP([2,3],[3,2]), MAP([10],[3]), MAP([3,2], [2,3])
	],
	[
		0,1,2,3,4
	]
);
----
{{5=4}=0, {10=2}=1, {2=3, 3=2}=2, {10=3}=3, {3=2, 2=3}=4}

# Nested keys (map) not succesfull (duplicate)
statement error
SELECT MAP(
	[
		MAP([5],[4]), MAP([10],[2]), MAP([2,3],[3,2]), MAP([10],[3]), MAP([3,2], [2,3]), MAP([5],[4])
	],
	[
		0,1,2,3,4,5
	]
);

# Nested keys (map) not succesfull (null)
statement error
SELECT MAP(
	[
		MAP([5],[4]), MAP([10],[2]), MAP([2,3],[3,2]), NULL, MAP([3,2], [2,3])
	],
	[
		0,1,2,3,4
	]
);
