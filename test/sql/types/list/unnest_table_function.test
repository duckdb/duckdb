# issue 7927

statement ok
CREATE TABLE tbl AS
SELECT * FROM (VALUES
	('a', array[4, 5, 5], array[5, 7]),
	('b', array[2, 3], array[1, 2, 3, 4]),
	('c', array[2, 3], array[4])
) t(k, a,b);

query I
SELECT array(
	SELECT list_contains(b, ax)
	FROM UNNEST(a) ta(ax)
) ab_intersect
FROM tbl;
----
[false, true, true]
[true, true]
[false, false]

query IIII
SELECT k, a, b,
	ARRAY(
		SELECT DISTINCT ax
			FROM UNNEST(a) ta(ax)
			WHERE ax = any(b)
			ORDER BY ALL
	) ab_intersect
	FROM tbl;
----
a	[4, 5, 5]	[5, 7]	[5]
b	[2, 3]	[1, 2, 3, 4]	[2, 3]
c	[2, 3]	[4]	[]