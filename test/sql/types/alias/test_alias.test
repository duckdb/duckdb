statement ok
PRAGMA enable_verification

statement ok
CREATE TYPE alias AS VARCHAR;

#Enums with the same name shouldn't work
statement error
CREATE TYPE alias AS INTEGER;

# Delete ALIASs
statement ok
DROP TYPE alias;

#This won't work because alias was already deleted
statement error
