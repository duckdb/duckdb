#This will drop alias
statement ok
DROP TYPE IF EXISTS alias;

#Nothing to drop but won't break
statement ok
DROP TYPE IF EXISTS alias;

# We can create alias again
statement ok
CREATE TYPE alias AS VARCHAR;

statement ok
DROP TYPE IF EXISTS alias;
