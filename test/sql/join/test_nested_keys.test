2	[NULL]	[NULL]
3	[]	[]
5	[9, 10, 11]	[9, 10, 11]

query III
SELECT i, pk, fk FROM intlists RIGHT OUTER JOIN intlistdim ON intlistdim.pk=intlists.fk ORDER BY i
