        (VALUES (0), (5), (10), (15)) AS t1
        ASOF LEFT JOIN (VALUES (1), (6), (11), (16)) AS t2
        ON t2.col0 > t1.col0
)
SELECT *
FROM t
WHERE right_val BETWEEN 3 AND 12
