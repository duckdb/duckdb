# Create tables with large string values so that we have to swizzle strings
# The tables have a single overlapping value to keep the join result small
statement ok
create table t1 as select concat('000000000000', range::VARCHAR) i from range(20000)

statement ok
create table t2 as select concat('000000000000', range::VARCHAR) j from range(19999, 39999)

query II
