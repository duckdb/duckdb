query IIII
WITH abc AS (
	SELECT * FROM (
    	VALUES (1, 'x'), (1, 'x'), (1, '0'), (1, '0')
    ) AS tbl(col0, col1)
)
SELECT x.col0 AS c1, x.col1 AS c2, y.col0 AS c3, y.col1 AS c4
FROM abc x JOIN abc y ON x.col0 = y.col0
	AND (x.col1 IS DISTINCT FROM y.col1)
ORDER BY c1, c2, c3, c4;
----
1	0	1	x
1	0	1	x
1	0	1	x
1	0	1	x
1	x	1	0
1	x	1	0
1	x	1	0
1	x	1	0

