statement ok
CREATE TABLE u(u_k0 TINYINT);

statement ok
INSERT INTO u VALUES (-128), (127);

query II
SELECT t_k0, u_k0 FROM   t, u   WHERE t_k0 = u_k0;
----
-128	 -128
127	127

statement ok
DROP TABLE t;

statement ok
DROP TABLE u;

