# unbound parameter with lists and NULL
statement ok
PREPARE v9 AS SELECT [?, NULL]

query I
EXECUTE v9(27)
----
[27, NULL]

query I
EXECUTE v9('hello world')
----
[hello world, NULL]

query I
EXECUTE v9(NULL)
----
