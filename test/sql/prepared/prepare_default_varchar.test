# unbound parameter with structs
statement ok
PREPARE v10 AS SELECT {'x': ?}

query I
EXECUTE v10(27)
----
{'x': 27}

query I
EXECUTE v10('hello world')
----
{'x': hello world}

query I
EXECUTE v10(NULL)
----
