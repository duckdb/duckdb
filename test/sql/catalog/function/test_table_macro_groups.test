CREATE TABLE car_pool (
  -- define columns (name / type / default value / nullable)
  id           DECIMAL      ,
  producer     VARCHAR(50)  ,
  model        VARCHAR(50)  ,
  yyyy         DECIMAL       CHECK (yyyy BETWEEN 1970 AND 2020),
  counter      DECIMAL       CHECK (counter >= 0),
  CONSTRAINT   car_pool_pk PRIMARY KEY (id)
);

