statement ok
INSERT INTO t1 VALUES (3, 3), (1, 1);

statement ok
CREATE TABLE t2 (y INT, z INT);

statement ok
INSERT INTO t2 VALUES (2, 2), (4, 4);

query III
SELECT x, y FROM t1 UNION ALL BY NAME SELECT y, z FROM t2;
----
