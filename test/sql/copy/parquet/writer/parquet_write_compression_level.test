require parquet

statement ok
PRAGMA enable_verification

statement ok
CREATE TABLE integers AS FROM range(100) t(i)

statement error
COPY integers TO '__TEST_DIR__/compress_level.parquet' (FORMAT 'parquet', COMPRESSION_LEVEL 10);
