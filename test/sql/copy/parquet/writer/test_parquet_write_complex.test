# name: test/sql/copy/parquet/writer/test_parquet_write_complex.test
# description: Parquet read and re-write various files
# group: [writer]

require parquet

# alltypes_dictionary: scan as parquet
query I nosort alltypes_dictionary
SELECT * FROM parquet_scan('${TEST_DATA_LOC}/parquet-testing/arrow/alltypes_dictionary.parquet');
----

# rewrite the file
statement ok
COPY (SELECT * FROM parquet_scan('${TEST_DATA_LOC}/parquet-testing/arrow/alltypes_dictionary.parquet')) TO '${TEST_TEMP_LOC}/alltypes_dictionary.parquet' (FORMAT 'PARQUET')

# verify that the rewritten file has the same values again
query I nosort alltypes_dictionary
SELECT * FROM parquet_scan('${TEST_TEMP_LOC}/alltypes_dictionary.parquet');
----

# bug687_nulls.parquet
query I nosort bug687_nulls
SELECT * FROM parquet_scan('${TEST_DATA_LOC}/parquet-testing/bug687_nulls.parquet') LIMIT 10;
----

statement ok
COPY (SELECT * FROM parquet_scan('${TEST_DATA_LOC}/parquet-testing/bug687_nulls.parquet')) TO '${TEST_TEMP_LOC}/bug687_nulls.parquet' (FORMAT 'PARQUET')

query I nosort bug687_nulls
SELECT * FROM parquet_scan('${TEST_TEMP_LOC}/bug687_nulls.parquet') LIMIT 10;
----

# Issue #1637: booleans encoded incorrectly
statement ok
COPY (SELECT true as x UNION ALL SELECT true) TO '${TEST_TEMP_LOC}/bug1637_booleans.parquet' (FORMAT 'PARQUET');

# Prior to the #1637 fix, duckdb wrote a parquet file containing true, false
query I
SELECT COUNT(*) FROM parquet_scan('${TEST_TEMP_LOC}/bug1637_booleans.parquet') WHERE x;
----
2

