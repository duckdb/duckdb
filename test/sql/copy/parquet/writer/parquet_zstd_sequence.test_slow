# name: test/sql/copy/parquet/writer/parquet_zstd_sequence.test_slow
# description: Test writing of large blobs into parquet files
# group: [writer]

require parquet

require 64bit

statement ok
COPY (SELECT * FROM read_csv_auto('{DATA_DIR}/csv/sequences.csv.gz', delim=',', header=True) LIMIT 25000) TO '{TEMP_DIR}/duckseq.parquet' (FORMAT 'PARQUET', CODEC 'ZSTD', ROW_GROUP_SIZE 25000);

query IIIIII
select count(*), min(strain), max(strain), min(strlen(sequence)), max(strlen(sequence)), avg(strlen(sequence))
from '{TEMP_DIR}/duckseq.parquet';
----
25000	AUS/NT01/2020	canine/HKG/20-03695/2020	17340	30018	29855.647080

statement ok
COPY
(
	SELECT lstrain::VARCHAR[] lstrain, lsequence::VARCHAR[] lsequence FROM (VALUES ([], []), (NULL, NULL), ([], [])) tbl(lstrain, lsequence)
	UNION ALL
	SELECT * FROM (
		SELECT LIST(strain) AS lstrain, LIST(sequence) AS lsequence FROM '{TEMP_DIR}/duckseq.parquet' LIMIT 10000
	)
	UNION ALL
	SELECT * FROM (VALUES ([], []), (NULL, NULL), ([], []))
)
TO '{TEMP_DIR}/duckseq2.parquet' (FORMAT 'PARQUET', CODEC 'ZSTD');

query I
SELECT COUNT(*) FROM '{TEMP_DIR}/duckseq2.parquet'
----
7

query IIIIII nosort querylabel
