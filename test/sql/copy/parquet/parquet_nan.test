# name: test/sql/copy/parquet/parquet_nan.test
# description: Read a parquet file with an invalid float value (infinity)
# group: [parquet]

require parquet

query TTT
select * from parquet_scan('{DATA_DIR}/parquet-testing/nan-float.parquet') order by 1
----
-1	foo	1
2.5	baz	1
inf	bar	0

query II
select * from parquet_scan('{DATA_DIR}/parquet-testing/arrow_nan.parquet', can_have_nan := true) where f='nan';
----
nan	nan

query II
select * from parquet_scan('{DATA_DIR}/parquet-testing/arrow_nan.parquet', can_have_nan := true) where f>10;
----
nan	nan

query II
select * from parquet_scan('{DATA_DIR}/parquet-testing/arrow_nan.parquet', can_have_nan := true) where d='nan';
----
nan	nan

query II
select * from parquet_scan('{DATA_DIR}/parquet-testing/arrow_nan.parquet', can_have_nan := true) where d>10;
----
nan	nan
