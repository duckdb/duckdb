<REGEX>:.*multi_partitioned_test.*partition_key=0.partition_key2=1.*[.]parquet	416	<REGEX>:\d+	<REGEX>:\d+	{'"val"'={max=2494, min=4, null_count=0}}	{partition_key=0, partition_key2=1}
<REGEX>:.*multi_partitioned_test.*partition_key=0.partition_key2=2.*[.]parquet	417	<REGEX>:\d+	<REGEX>:\d+	{'"val"'={max=2498, min=2, null_count=0}}	{partition_key=0, partition_key2=2}
<REGEX>:.*multi_partitioned_test.*partition_key=1.partition_key2=0.*[.]parquet	417	<REGEX>:\d+	<REGEX>:\d+	{'"val"'={max=2499, min=3, null_count=0}}	{partition_key=1, partition_key2=0}
