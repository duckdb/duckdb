<REGEX>:.*multi_partitioned_test.*partition_key=1.partition_key2=2.*[.]parquet	416	<REGEX>:\d+	<REGEX>:\d+	{'"val"'={max=2495, min=5, null_count=0}}	{partition_key=1, partition_key2=2}

