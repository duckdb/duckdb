# name: test/sql/copy/csv/parallel/csv_parallel_clickbench.test_slow
# description: Test parallel read CSV function on Clickbench
# group: [parallel]

require parquet

require httpfs

statement ok
SET experimental_parallel_csv=true;

statement ok
pragma threads=4

statement ok
