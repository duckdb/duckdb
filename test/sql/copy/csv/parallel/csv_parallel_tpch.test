# description: Test read CSV function on TPCH
# group: [parallel]

require tpch

#statement ok
#pragma verify_external

statement ok
SET experimental_parallel_csv=true;

statement ok
pragma threads=4

statement ok
CALL dbgen(sf=1, suffix='_og');

foreach tpch_tbl customer lineitem nation orders part partsupp region supplier

statement ok
COPY ${tpch_tbl}_og TO '__TEST_DIR__/${tpch_tbl}.csv';

statement ok
create table ${tpch_tbl} as select * from ${tpch_tbl}_og limit 0;

statement ok
copy ${tpch_tbl} from '__TEST_DIR__/${tpch_tbl}.csv';

query I
select count(*) from (SELECT * FROM ${tpch_tbl} EXCEPT SELECT * FROM ${tpch_tbl}_og);
----
0

query I
select count (*) from (SELECT * FROM ${tpch_tbl}_og EXCEPT SELECT * FROM ${tpch_tbl});
----
0

endloop

