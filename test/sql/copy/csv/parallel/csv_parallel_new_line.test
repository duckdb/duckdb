# name: test/sql/copy/csv/parallel/csv_parallel_new_line.test
# description: Test parallel read CSV function with different settings of new lines
# group: [parallel]

statement ok
SET experimental_parallel_csv=true;

# force parallelism of the queries
statement ok
PRAGMA verify_parallelism

# Test read_csv with user defined variable

loop thr 1 8

loop i 25 100

statement ok
pragma threads=${thr}

# Test read_csv auto with \n
query IIII
select * from read_csv_auto('test/sql/copy/csv/data/test/multi_column_string.csv', buffer_size=${i})
