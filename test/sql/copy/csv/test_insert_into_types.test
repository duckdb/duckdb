# No pushdown with mid-ops
statement ok
CREATE TABLE ppl (
        name VARCHAR
);

statement ok
insert into ppl values ('alice'), ('bob'), ('pedro')

statement error
INSERT INTO proj
SELECT ppl.name,id
FROM read_csv('{DATA_DIR}/csv/file_error.csv') T inner join ppl on (ppl.name = T.name);
----
Could not convert string '3r' to INT32

statement error
