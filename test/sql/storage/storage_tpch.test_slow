# name: test/sql/storage/storage_tpch.test_slow
# description: Test TPC-H SF0.1
# group: [storage]

require tpch

# load the DB from disk
load {LOCAL_TEMP_DIR}/store_tpch.db

statement ok
CALL dbgen(sf=0.1);

loop i 1 9

query I
PRAGMA tpch(${i})
----
<FILE>:extension/tpch/dbgen/answers/sf0.1/q0${i}.csv

endloop

loop i 10 23

query I
PRAGMA tpch(${i})
----
<FILE>:extension/tpch/dbgen/answers/sf0.1/q${i}.csv

endloop

# restart and run all queries again
restart

loop i 1 9

query I
PRAGMA tpch(${i})
----
<FILE>:extension/tpch/dbgen/answers/sf0.1/q0${i}.csv

endloop

loop i 10 23

query I
PRAGMA tpch(${i})
----
<FILE>:extension/tpch/dbgen/answers/sf0.1/q${i}.csv

endloop
