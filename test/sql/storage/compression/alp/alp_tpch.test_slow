	l_shipdate DATE NOT NULL,
	l_commitdate DATE NOT NULL,
	l_receiptdate DATE NOT NULL,
	l_shipinstruct VARCHAR NOT NULL,
	l_shipmode VARCHAR NOT NULL,
	l_comment VARCHAR NOT NULL
);

statement ok
