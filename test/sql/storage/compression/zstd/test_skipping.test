create table tbl as
select
	i // 5_000 as num,
	num::VARCHAR || list_reduce([uuid()::varchar for x in range(10)], (x, y) -> concat(x, y)) str
from range(20_000) t(i) order by num

statement ok
force checkpoint;

query I
select str[0:1]::BIGINT from tbl where num = 1 limit 10;
