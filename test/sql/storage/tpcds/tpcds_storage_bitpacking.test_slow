# name: test/sql/storage/tpcds/tpcds_storage_bitpacking.test_slow
# description: Test TPC-DS SF1 queries on persistent storage
# group: [tpcds]

require tpcds

load __TEST_DIR__/tpcds_storage.db

statement ok
PRAGMA force_compression='bitpacking'

statement ok
call dsdgen(sf=1);

statement ok
CHECKPOINT

restart

statement ok
PRAGMA default_null_order='NULLS LAST'

# too slow queries:
# 64, 85

loop i 1 9

query I
PRAGMA tpcds(${i})
----
<FILE>:extension/tpcds/dsdgen/answers/sf1/0${i}.csv

endloop

loop i 10 64

query I
PRAGMA tpcds(${i})
----
<FILE>:extension/tpcds/dsdgen/answers/sf1/${i}.csv

endloop

loop i 65 85

query I
PRAGMA tpcds(${i})
----
<FILE>:extension/tpcds/dsdgen/answers/sf1/${i}.csv

endloop

loop i 86 99

query I
PRAGMA tpcds(${i})
----
<FILE>:extension/tpcds/dsdgen/answers/sf1/${i}.csv

endloop
