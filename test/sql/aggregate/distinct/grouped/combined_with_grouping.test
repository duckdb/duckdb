statement ok
create table students (
	course VARCHAR,
	type VARCHAR,
	value BIGINT
);

statement ok
insert into students
		(course, type, value)
	values
		('CS', 'Bachelor', 34),
		('CS', 'Bachelor', 34),
		('CS', 'PhD', 12),
		('Math', 'Masters', 12),
		('CS', NULL, 10),
		('CS', NULL, 12),
		('Math', NULL, 12),
		('Math', NULL, NULL);

