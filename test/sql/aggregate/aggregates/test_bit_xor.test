query IIII
SELECT BIT_XOR(i), BIT_XOR(1), BIT_XOR(DISTINCT i), BIT_XOR(NULL) FROM integers
----
24
0
20
NULL

# test on an empty set
query I
SELECT BIT_XOR(i) FROM integers WHERE i > 100
----
NULL

