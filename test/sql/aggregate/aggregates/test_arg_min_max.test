statement ok
create table args (a integer, b integer)

statement ok
insert into args values (1,1), (2,2), (8,8), (10,10)

query II
select argmin(a,b), argmax(a,b) from args;
----
1.000000	10.000000

query II
select argmin(a,b), argmax(a,b) from args group by a%2 ORDER BY argmin(a,b);
----
1	1
2	10

