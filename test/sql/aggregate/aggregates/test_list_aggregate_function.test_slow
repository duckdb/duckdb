# INTEGER types

foreach type tinyint smallint integer bigint utinyint usmallint uinteger ubigint

statement ok
CREATE TABLE test (g INTEGER, i ${type})

query I
SELECT LIST(i) FROM test GROUP BY g ORDER BY g
----

statement ok
INSERT INTO test VALUES (1, 2), (1, 3), (2, 4)

query I
SELECT LIST(i) FROM test GROUP BY g ORDER BY g
----
[2, 3]
[4]

statement ok
DROP TABLE test

endloop

