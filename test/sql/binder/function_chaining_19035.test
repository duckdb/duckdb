statement ok
PRAGMA enable_verification

query I
with stage as (
      select
          [1,2,3] as c1,
          [4,5,6] as c2
)
select
     list_transform(
       c1,
       lambda x: list_bool_or(
          list_transform(
            c2,
            lambda y: y == x
          )
       )
     ) as c3
from stage;
----
[false, false, false]

query I
select list_transform(
           [1,2,3],
           lambda x: list_max(
                list_transform([4,5,6], lambda y: y + x)
           )) as result;
----
[7, 8, 9]

