# issue 16094
statement ok
copy (
	select
		x
	from generate_series(1,100) as g(x)
) to '{TEMP_DIR}/16094.parquet'
with (
	field_ids {x: 1}
);

statement ok
select
	x,
	filename
from read_parquet(
	'{TEMP_DIR}/16094.parquet',
	schema=map {
		1: {name: 'x', type: 'int', default_value: NULL}
	},
	filename=True
) where x = 1;
