# name: ${FILE_PATH}
# description: ${DESCRIPTION}
# group: [ingestion]

name Q${table_name}
group ingestion
subgroup tpcds

require tpcds

require parquet

cache tpcds_sf100_ingest.duckdb

load benchmark/large/ingestion/tpcds/load_tpcds_sf100.sql

# reads table from view created in load script
run
create table ${table_name}_ingested as select * from ${table_name}_${format};

cleanup
drop table ${table_name}_ingested;