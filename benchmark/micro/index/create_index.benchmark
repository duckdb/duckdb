# name: benchmark/micro/index/create_index.benchmark
# description: Create index on 10000000 integer tuples
# group: [index]

load
CREATE TABLE integers AS SELECT (i * 9876983769044 % 10000000) AS i FROM range(0, 10000000) t(i);

run
CREATE INDEX i_index ON integers using art(i);

cleanup
DROP INDEX i_index;
