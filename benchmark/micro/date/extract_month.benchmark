# name: benchmark/micro/date/extract_month.benchmark
# description: EXTRACT(month from date)
# group: [date]

name Extract Month
group date

load
CREATE TABLE dates AS SELECT DATE '1992-01-01' + (i % 10000)::INT AS d FROM range(0, 1000000000) tbl(i);

run
SELECT MIN(EXTRACT(MONTH FROM d)) FROM dates

result I
1
